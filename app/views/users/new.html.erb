<h1>登録</h1>

<%= form_for @user do |f| %>

<p>
	<%= f.label :email %><br>
	<%= f.text_field :email %><br>
	<%= f.label :password %><br>
	<%= f.text_field :password %><br>
	<%= f.label :password_confirmation %><br>
	<%= f.text_field :password_confirmation %>

</p>

<p>
	<%= f.submit %>
</p>

<% end %>